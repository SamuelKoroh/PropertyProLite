{
  "name": "propertyprolite",
  "version": "1.0.0",
  "description": "Property Pro Lite is a platform where people can create and/or search properties for sale or rent.",
  "main": "index.js",
  "scripts": {
    "start": "node ./build-server/index.js",
    "build": "babel -d ./build-server ./server",
    "dev": "nodemon --watch server --exec babel-node ./server/index.js",
    "test:server": "./node_modules/.bin/mocha --timeout 500000 --require @babel/register server/tests/",
    "test": "nyc npm run test:server && nyc report --reporter=lcov --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamuelKoroh/PropertyProLite.git"
  },
  "author": "Koroh Samuel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SamuelKoroh/PropertyProLite/issues"
  },
  "homepage": "https://github.com/SamuelKoroh/PropertyProLite#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-airbnb": "^3.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jasmine": "^3.4.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "helmet": "^3.18.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
